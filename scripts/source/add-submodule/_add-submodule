#compdef add-submodule

# Zsh completion for add-submodule
# Generated automatically - customize as needed

_add-submodule() {
    local context state line
    typeset -A opt_args
    
    _arguments -C \
        '(-h --help)'{-h,--help}'[Show help information]' \
        '(-v --version)'{-v,--version}'[Show version information]' \
        '*::arg:_files'
}

_add-submodule "$@"
